<% include head %>
<header>
<% include nav %>
</header>

<body>
<div id="photos-wrapper">
	<h1>A node.js voting game - is it electric?</h1>
	<div id="photo">
		<img src="images/<%=carImage%>">
	</div>
</div>

<div id="button-holder">
	<form class="vote-button" action="./electric" method="post">
		<input type="hidden" name="photo" value="<%=carImage%>">
		<input type="submit" class="btn btn-info" value="Electric!">
	</form>
	<form class="vote-button" action="./nonelectric" method="post">
		<input type="hidden" name="photo" value="<%=carImage%>">
		<input type="submit" class="btn btn-info" value="Not Electric!">
	</form>
</div>

<% include footer %>
</body>
